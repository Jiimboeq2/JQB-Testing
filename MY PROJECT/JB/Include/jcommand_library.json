{
  "version": "2.0.0",
  "description": "J Command Library Reference - Complete documentation of all available commands",
  "last_updated": "2025-10-19",
  
  "command_categories": {
    
    "message_utility": {
      "description": "Messages, announcements, and utility functions",
      "commands": {
        "AuthorMessage": {
          "function": "JCmd_AuthorMessage",
          "description": "Display formatted message to user",
          "parameters": [
            {"name": "message", "type": "string", "required": true, "description": "Message to display"}
          ],
          "usage_example": "call JCmd_AuthorMessage \"Boss 1 Starting\"",
          "json_example": {
            "command": "QuestBot: Author_Message",
            "parameters": ["Starting boss encounter"]
          }
        },
        "Echo": {
          "function": "JCmd_Echo",
          "description": "Echo message to OgreBot console",
          "parameters": [
            {"name": "message", "type": "string", "required": true}
          ]
        },
        "Announce": {
          "function": "JCmd_Announce",
          "description": "Announce message to all characters",
          "parameters": [
            {"name": "message", "type": "string", "required": true}
          ]
        },
        "Checkpoint": {
          "function": "JCmd_Checkpoint",
          "description": "Create checkpoint marker with message",
          "parameters": [
            {"name": "message", "type": "string", "required": true}
          ],
          "json_example": {
            "command": "QuestBot: Checkpoint",
            "parameters": ["Boss 1 Complete"]
          }
        },
        "Summon": {
          "function": "JCmd_Summon",
          "description": "Summon pets/group members",
          "parameters": []
        }
      }
    },
    
    "zone": {
      "description": "Zone management and validation",
      "commands": {
        "ConfirmZone": {
          "function": "JCmd_ConfirmZone",
          "description": "Verify current zone matches expected zone, stops execution if mismatch",
          "parameters": [
            {"name": "expectedZone", "type": "string", "required": true, "description": "Expected zone name"}
          ],
          "json_example": {
            "command": "QuestBot: Confirm_ZoneName",
            "parameters": ["all", "Kralet Penumbra"]
          }
        },
        "WaitForZone": {
          "function": "JCmd_WaitForZone",
          "description": "Wait for specific zone to load",
          "parameters": [
            {"name": "zoneShortName", "type": "string", "required": true},
            {"name": "timeout", "type": "int", "required": false, "default": 60}
          ],
          "json_example": {
            "command": "QuestBot: WaitForZone",
            "parameters": ["qeynos_province02", "60"]
          }
        },
        "Zone": {
          "function": "JCmd_Zone",
          "description": "Zone out/use zone door",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "Evac": {
          "function": "JCmd_Evac",
          "description": "Emergency evacuation",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "Revive": {
          "function": "JCmd_Revive",
          "description": "Revive dead characters",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        }
      }
    },
    
    "wait": {
      "description": "Wait/delay commands",
      "commands": {
        "Wait": {
          "function": "JCmd_Wait",
          "description": "Wait for specified seconds",
          "parameters": [
            {"name": "seconds", "type": "int", "required": true, "description": "Number of seconds to wait"}
          ],
          "json_example": {
            "command": "QuestBot: Wait",
            "parameters": ["5"]
          }
        },
        "WaitWhileInCombat": {
          "function": "JCmd_WaitWhileInCombat",
          "description": "Wait until combat ends",
          "parameters": []
        },
        "WaitForCombat": {
          "function": "JCmd_WaitForCombat",
          "description": "Wait for combat to start",
          "parameters": [
            {"name": "timeout", "type": "int", "required": false, "default": 30}
          ]
        },
        "WaitForActorExists": {
          "function": "JCmd_WaitForActorExists",
          "description": "Wait for actor to spawn",
          "parameters": [
            {"name": "actorName", "type": "string", "required": true},
            {"name": "timeout", "type": "int", "required": false, "default": 30}
          ]
        },
        "WaitForActorDead": {
          "function": "JCmd_WaitForActorDead",
          "description": "Wait for actor to die",
          "parameters": [
            {"name": "actorName", "type": "string", "required": true},
            {"name": "timeout", "type": "int", "required": false, "default": 300}
          ],
          "json_example": {
            "command": "QuestBot: Conditional_Loop",
            "parameters": ["all", "ActorDead", "Boss Name", "300"]
          }
        }
      }
    },
    
    "ogrebot_core": {
      "description": "Core OgreBot commands",
      "commands": {
        "LetsGo": {
          "function": "JCmd_OgreBot_LetsGo",
          "description": "Start OgreBot automation",
          "parameters": [
            {"name": "who", "type": "string", "required": true, "options": ["all", "character_name", "tanks", "healers", "dps"]}
          ]
        },
        "Assist": {
          "function": "JCmd_OgreBot_Assist",
          "description": "Set assist target",
          "parameters": [
            {"name": "assistTarget", "type": "string", "required": true},
            {"name": "who", "type": "string", "required": false, "default": "all"}
          ]
        },
        "CampSpot": {
          "function": "JCmd_OgreBot_Campspot",
          "description": "Set camp spot coordinates",
          "parameters": [
            {"name": "who", "type": "string", "required": true},
            {"name": "x", "type": "float", "required": true},
            {"name": "y", "type": "float", "required": true},
            {"name": "z", "type": "float", "required": true}
          ],
          "json_example": {
            "command": "OgreBot: Campspot",
            "parameters": ["all", "100.5", "50.2", "-200.3"]
          }
        },
        "SetCampspot": {
          "function": "JCmd_OgreBot_SetCampspot",
          "description": "Set camp spot to current position",
          "parameters": []
        },
        "ClearCampspot": {
          "function": "JCmd_OgreBot_ClearCampspot",
          "description": "Clear camp spot",
          "parameters": [
            {"name": "who", "type": "string", "required": false, "default": "all"}
          ]
        },
        "ReLd_Bot": {
          "function": "JCmd_OgreBot_ReLd_Bot",
          "description": "Reload OgreBot",
          "parameters": [
            {"name": "who", "type": "string", "required": false, "default": "all"}
          ],
          "json_example": {
            "command": "OgreBot: reload_bot",
            "parameters": ["all"]
          }
        },
        "Pause": {
          "function": "JCmd_OgreBot_Pause",
          "description": "Pause OgreBot",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "Resume": {
          "function": "JCmd_OgreBot_Resume",
          "description": "Resume OgreBot",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "Attack": {
          "function": "JCmd_OgreBot_Attack",
          "description": "Start attacking",
          "parameters": [
            {"name": "who", "type": "string", "required": false, "default": "all"}
          ],
          "json_example": {
            "command": "OgreBot: Attack",
            "parameters": ["all"]
          }
        },
        "EndCombat": {
          "function": "JCmd_OgreBot_EndCombat",
          "description": "End combat/stop attacking",
          "parameters": [
            {"name": "who", "type": "string", "required": false, "default": "all"}
          ]
        }
      }
    },
    
    "autotarget": {
      "description": "Auto-target system commands",
      "commands": {
        "AddAutoTarget": {
          "function": "JCmd_AutoTarget_AddActor",
          "description": "Add mob to auto-target list",
          "parameters": [
            {"name": "who", "type": "string", "required": true},
            {"name": "actorName", "type": "string", "required": true},
            {"name": "priority", "type": "int", "required": false, "default": 0, "description": "0 = highest priority"}
          ],
          "json_example": {
            "command": "OgreBot: AddAutoTargetByName",
            "parameters": ["all", "a trash mob", "0"]
          }
        },
        "ClearAutoTarget": {
          "function": "JCmd_AutoTarget_Clear",
          "description": "Clear auto-target list",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "EnableAutoTarget": {
          "function": "JCmd_AutoTarget_Enable",
          "description": "Enable auto-targeting",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "DisableAutoTarget": {
          "function": "JCmd_AutoTarget_Disable",
          "description": "Disable auto-targeting",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "SetScanRadius": {
          "function": "JCmd_AutoTarget_ScanRadius",
          "description": "Set auto-target scan radius",
          "parameters": [
            {"name": "who", "type": "string", "required": true},
            {"name": "radius", "type": "int", "required": true}
          ]
        }
      }
    },
    
    "abilities": {
      "description": "Ability and casting commands",
      "commands": {
        "CastAbility": {
          "function": "JCmd_CastAbility",
          "description": "Cast specific ability",
          "parameters": [
            {"name": "who", "type": "string", "required": true},
            {"name": "abilityName", "type": "string", "required": true}
          ]
        },
        "CastAbilityNoChecks": {
          "function": "JCmd_CastAbilityNoChecks",
          "description": "Cast ability bypassing normal checks",
          "parameters": [
            {"name": "who", "type": "string", "required": true},
            {"name": "abilityName", "type": "string", "required": true}
          ]
        },
        "CancelCasting": {
          "function": "JCmd_CancelCasting",
          "description": "Cancel current casting",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "UseItem": {
          "function": "JCmd_UseItem",
          "description": "Use item from inventory",
          "parameters": [
            {"name": "who", "type": "string", "required": true},
            {"name": "itemName", "type": "string", "required": true}
          ]
        }
      }
    },
    
    "ui": {
      "description": "UI manipulation commands",
      "commands": {
        "ChangeUI": {
          "function": "JCmd_ChangeUI",
          "description": "Change OgreBot UI setting",
          "parameters": [
            {"name": "who", "type": "string", "required": true},
            {"name": "control", "type": "string", "required": true},
            {"name": "value", "type": "string", "required": true}
          ]
        },
        "EnableCheckbox": {
          "function": "JCmd_EnableCheckbox",
          "description": "Enable UI checkbox",
          "parameters": [
            {"name": "who", "type": "string", "required": true},
            {"name": "checkboxName", "type": "string", "required": true}
          ]
        },
        "DisableCheckbox": {
          "function": "JCmd_DisableCheckbox",
          "description": "Disable UI checkbox",
          "parameters": [
            {"name": "who", "type": "string", "required": true},
            {"name": "checkboxName", "type": "string", "required": true}
          ]
        }
      }
    },
    
    "movement": {
      "description": "Movement control commands",
      "commands": {
        "EnableMoveToArea": {
          "function": "JCmd_EnableMoveToArea",
          "description": "Enable move to area",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "DisableMoveToArea": {
          "function": "JCmd_DisableMoveToArea",
          "description": "Disable move to area",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "EnableMoveBehind": {
          "function": "JCmd_EnableMoveBehind",
          "description": "Enable move behind",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "DisableMoveBehind": {
          "function": "JCmd_DisableMoveBehind",
          "description": "Disable move behind",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "JoustOut": {
          "function": "JCmd_JoustOut",
          "description": "Execute joust out",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        },
        "JoustIn": {
          "function": "JCmd_JoustIn",
          "description": "Execute joust in",
          "parameters": [
            {"name": "who", "type": "string", "required": true}
          ]
        }
      }
    },
    
    "interaction": {
      "description": "NPC and actor interaction commands",
      "commands": {
        "Hail": {
          "function": "JCmd_Hail",
          "description": "Hail an NPC",
          "parameters": [
            {"name": "who", "type": "string", "required": true},
            {"name": "npcName", "type": "string", "required": true}
          ],
          "json_example": {
            "command": "QuestBot: Hail",
            "parameters": ["Merchant Bob"]
          }
        },
        "ApplyVerb": {
          "function": "JCmd_ApplyVerb",
          "description": "Apply verb to actor (open, activate, etc)",
          "parameters": [
            {"name": "who", "type": "string", "required": true},
            {"name": "actorName", "type": "string", "required": true},
            {"name": "verbName", "type": "string", "required": true}
          ],
          "json_example": {
            "command": "QuestBot: Apply_Verb",
            "parameters": ["all", "a treasure chest", "Open"]
          }
        },
        "DoubleClick": {
          "function": "JCmd_DoubleClick",
          "description": "Double-click an actor",
          "parameters": [
            {"name": "actorName", "type": "string", "required": true}
          ],
          "json_example": {
            "command": "QuestBot: Actor_DoubleClick",
            "parameters": ["all", "a door"]
          }
        },
        "Target": {
          "function": "JCmd_Target",
          "description": "Target actor or clear target",
          "parameters": [
            {"name": "targetName", "type": "string", "required": true, "description": "Actor name or 'none' to clear"}
          ]
        }
      }
    },
    
    "navigation": {
      "description": "Navigation and pathing commands",
      "commands": {
        "Navigate": {
          "function": "JCmd_Navigate",
          "description": "Navigate through waypoint path",
          "parameters": [
            {"name": "pathName", "type": "string", "required": true},
            {"name": "reverse", "type": "bool", "required": false, "default": "false"},
            {"name": "preBuff", "type": "bool", "required": false, "default": "false"},
            {"name": "ignoreAggro", "type": "bool", "required": false, "default": "false"}
          ],
          "json_example": {
            "command": "QuestBot: Point to Point Navigation",
            "parameters": ["all", "path_to_boss1", "5"]
          }
        },
        "GoTo": {
          "function": "JCmd_GoTo",
          "description": "Navigate to specific coordinates",
          "parameters": [
            {"name": "x", "type": "float", "required": true},
            {"name": "y", "type": "float", "required": true},
            {"name": "z", "type": "float", "required": true},
            {"name": "tolerance", "type": "float", "required": false, "default": 3}
          ]
        }
      }
    },
    
    "quest": {
      "description": "Quest and reward commands",
      "commands": {
        "QuestCheck": {
          "function": "JCmd_QuestCheck",
          "description": "Check quest step status",
          "parameters": [
            {"name": "questName", "type": "string", "required": true},
            {"name": "stepText", "type": "string", "required": true},
            {"name": "expectedState", "type": "string", "required": false, "default": "unchecked"}
          ]
        },
        "AcceptReward": {
          "function": "JCmd_AcceptReward",
          "description": "Accept quest reward",
          "parameters": [
            {"name": "who", "type": "string", "required": true},
            {"name": "rewardNum", "type": "string", "required": false, "default": "1"}
          ]
        }
      }
    },
    
    "debug": {
      "description": "Debug and utility commands",
      "commands": {
        "DebugPosition": {
          "function": "JCmd_DebugPosition",
          "description": "Display current position info",
          "parameters": []
        },
        "DebugTarget": {
          "function": "JCmd_DebugTarget",
          "description": "Display current target info",
          "parameters": []
        },
        "ShowStats": {
          "function": "JCmd_ShowStats",
          "description": "Display character stats",
          "parameters": []
        }
      }
    }
  },
  
  "parameter_types": {
    "who_targets": {
      "description": "Valid 'who' parameter values",
      "values": [
        "all",
        "${Me.Name}",
        "tanks",
        "healers",
        "dps",
        "casters",
        "melee",
        "Fighter",
        "Priest",
        "Scout",
        "Mage",
        "igw:${Me.Name}"
      ]
    },
    "class_names": {
      "tanks": ["guardian", "berserker", "monk", "bruiser", "paladin", "shadowknight"],
      "healers": ["warden", "fury", "templar", "inquisitor", "mystic", "defiler", "channeler"],
      "scouts": ["ranger", "assassin", "brigand", "swashbuckler", "troubador", "dirge"],
      "mages": ["wizard", "warlock", "conjuror", "necromancer", "coercer", "illusionist"]
    }
  }
}